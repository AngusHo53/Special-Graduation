<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rabbit.login">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CommonUser"
            android:label="@string/title_activity_add_member"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SelectAccountType"
            android:label="@string/title_activity_select_account_type"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SeminarOrganizers"
            android:label="@string/title_activity_seminar_organizers"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".meetinglist"
            android:label="@string/title_activity_meetinglist"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>